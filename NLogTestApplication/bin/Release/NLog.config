<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true"
      internalLogLevel="Warn" internalLogFile="c:\temp\nlog-internal.log">
  <extensions>
    <add assembly="NLog.Target.Custom"/>
  </extensions>

  <targets>
    <target  xsi:type="AzureAppendBlob" name="blobStorage" layout="${longdate}|${level:uppercase=true}|${logger}|${message}" ConnectionString="DefaultEndpointsProtocol=https;AccountName=storagecontainerniharika;AccountKey=yOheOnoodbzSz9NU4GAgQL+C4w5CVjN16CNVhVaen6y9VzPT86spgNDY5OQq9mNk/KtnIlyFe7ggoSuvtqb45Q==;EndpointSuffix=core.windows.net" Container="nlogblobcontainer" BlobName="nlogblob"/>
    <target  xsi:type="AzureAppInsight" name="appInsight" layout="${longdate}|${level:uppercase=true}|${logger}|${message}" InstrumentationKey="7210f7c7-9afd-42db-95d2-62eb006b9ad4"/>
    <target  xsi:type="AzureTable" name="tableStorage" layout="${longdate}|${level:uppercase=true}|${logger}|${message}" ConnectionString="DefaultEndpointsProtocol=https;AccountName=storagecontainerniharika;AccountKey=yOheOnoodbzSz9NU4GAgQL+C4w5CVjN16CNVhVaen6y9VzPT86spgNDY5OQq9mNk/KtnIlyFe7ggoSuvtqb45Q==;EndpointSuffix=core.windows.net" TableName="nlogtable"/>
    <target  xsi:type="ColoredConsole" name="colored_console"/>
  </targets>

  <rules>
    <logger name="*" minlevel="Trace" writeTo="blobStorage"/>
    <logger name="*" minlevel="Trace" writeTo="appInsight"/>
    <logger name="*" minlevel="Trace" writeTo="tableStorage"/>
    <logger name="*" minlevel="Trace" writeTo="colored_console"/>
  </rules>
</nlog>




